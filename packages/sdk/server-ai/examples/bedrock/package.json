  "description": "LaunchDarkly AI SDK for Node.js",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node ./dist/index.js",
