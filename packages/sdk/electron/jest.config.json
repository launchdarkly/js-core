{
  "transform": { "^.+\\.ts?$": "ts-jest" },
  "testMatch": ["**/__tests__/**/*test.ts?(x)"],
  "testEnvironment": "node",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "collectCoverageFrom": ["src/**/*.ts"],
  "testPathIgnorePatterns": ["node_modules", "example", "dist"]
}
