{
  "name": "@launchdarkly/react-native-client-sdk",
  "version": "0.0.1",
  "description": "React Native LaunchDarkly SDK",
  "homepage": "https://github.com/launchdarkly/js-core/tree/main/packages/sdk/react-native",
  "repository": {
    "type": "git",
    "url": "https://github.com/launchdarkly/js-core.git"
  },
  "license": "Apache-2.0",
  "packageManager": "yarn@3.4.1",
  "keywords": [
    "launchdarkly",
    "react-native"
  ],
  "type": "commonjs",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "rb": "rollup -c --configPlugin typescript",
    "rbw": "yarn rb --watch",
    "build": "npx tsc && yarn link-dev",
    "tsw": "yarn tsc --watch",
    "start": "rimraf dist && yarn tsw",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write '**/*.@(js|ts|tsx|json|css)' --ignore-path ../../../.prettierignore",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --ci --runInBand",
    "coverage": "yarn test --coverage",
    "check": "yarn prettier && yarn lint && yarn build && yarn test",
    "link-dev": "./link-dev.sh"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "dependencies": {
    "@launchdarkly/js-client-sdk-common": "0.0.1",
    "base64-js": "^1.5.1",
    "event-target-shim": "^6.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.0",
    "@types/rollup-plugin-generate-package-json": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "launchdarkly-js-test-helpers": "^2.2.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "ts-jest": "^29.1.0",
    "typedoc": "0.25.0",
    "typescript": "5.1.6"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
