{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true,
    "skipLibCheck": true
  },
  "lib": ["ES6"],
  "exclude": ["**/*.test.ts", "dist", "node_modules"]
}
